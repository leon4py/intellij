<templateSet group="Java">
  <template name="tpvt" value="@Test&#10;public void test$METHOD_NAME$() throws Exception {&#10;    $END$&#10;}" description="" toReformat="false" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="bean" value="@Bean&#10;public $CLASS_NAME$ $VALUE$() {&#10;    return new $CLASS_NAME$();&#10;}" description="快速Bean" toReformat="false" toShortenFQNames="true">
